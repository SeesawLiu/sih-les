<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <components>
    <component
            id="ReportGen.Util"
            service="com.Sconit.Utility.Report.IReportGen, com.Sconit.Utility"
            type="com.Sconit.Utility.Report.ReportGen, com.Sconit.Utility" >
      <parameters>
        <ReportTemplateFolder>#{ReportTemplateFolder}</ReportTemplateFolder>
        <ReportTemporaryFolder>#{ReportTemporaryFolder}</ReportTemporaryFolder>
        <dicReportService>
            <dictionary keyType="System.String, mscorlib" valueType="System.Type, mscorlib">
                <!-- 外部要货单-->
                <entry key="ORD_Procurement.xls">com.Sconit.Utility.Report.Operator.RepPurchaseOrderOperator</entry>
                <!-- 内部要货单-->
                <entry key="ORD_Transfer.xls">com.Sconit.Utility.Report.Operator.RepTransferOderOperator</entry>
                <!-- 生产订单-->
                <entry key="ORD_Production.xls">com.Sconit.Utility.Report.Operator.RepProductionOrderOperator</entry>
                <!-- 拣货单-->
                <entry key="PickList.xls">com.Sconit.Utility.Report.Operator.RepPickListOrderOperator</entry>
                <!-- 条码单张-->
                <entry key="BarCode.xls">com.Sconit.Utility.Report.Operator.RepBarCodeOperator</entry>
                <!-- 收货单-->
                <entry key="REC_Receipt.xls">com.Sconit.Utility.Report.Operator.RepReceiptOrderOperator</entry>
                <!-- 采购送货单-->
                <entry key="ASN_Procurement.xls">com.Sconit.Utility.Report.Operator.RepASNProcurementOperator</entry>
                <!-- 移库送货单-->
                <entry key="ASN_Transfer.xls">com.Sconit.Utility.Report.Operator.RepASNTransferOperator</entry>
                <!-- 条码A4-->
                <entry key="BarCodeA4.xls">com.Sconit.Utility.Report.Operator.RepBarCodeA4Operator</entry>
                <!-- 总装生产单-->
                <entry key="AssemblyNote.xls">com.Sconit.Utility.Report.Operator.RepAssembleOrderOperator</entry>
                <!-- Kit单-->
                <entry key="KitOrder.xls">com.Sconit.Utility.Report.Operator.RepKitOrderOperator</entry>
                <!--计划外出入库单-->
                <entry key="MiscOrder.xls">com.Sconit.Utility.Report.Operator.RepMiscOrderOperator</entry>
                <!-- 排序单-->
                <entry key="SequenceOrder.xls">com.Sconit.Utility.Report.Operator.RepSequenceOrderOperator</entry>
                <!-- 看板卡-->
                <entry key="KanBanCard.xls">com.Sconit.Utility.Report.Operator.RepKanBanCardOperator</entry>
            </dictionary>
        </dicReportService>
      </parameters>
    </component>
  </components>
</configuration>